<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:gravity="center_horizontal"
    android:orientation="vertical">

    <com.jcs.where.widget.JcsTitle
        android:id="@+id/jcsTitle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:backIcon="@drawable/ic_back_black"
        app:middleTitle="@string/about_app_title"
        app:middleTitleColor="@color/black_333333"
        app:showBackIv="true"
        app:showBottomLine="true" />

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <ImageView
                android:layout_width="@dimen/dp_90"
                android:layout_height="@dimen/dp_90"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="@dimen/dp_25"
                android:scaleType="centerInside"
                android:src="@mipmap/ic_launcher_foreground" />


            <TextView
                android:id="@+id/version_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="@dimen/dp_15"
                android:textColor="@color/black_333333"
                android:textSize="@dimen/sp_19"
                tools:text="版本 0.0.1 Testversion" />


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_25"
                android:paddingStart="@dimen/dp_15"
                android:paddingEnd="@dimen/dp_15"
                android:text="@string/about_app_title"
                android:textColor="@color/black_333333"
                android:textSize="@dimen/sp_17"
                android:textStyle="bold" />


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_12"
                android:lineSpacingMultiplier="1.2"
                android:paddingStart="@dimen/dp_15"
                android:paddingEnd="@dimen/dp_15"
                android:text="@string/where_app_introduction"
                android:textColor="@color/grey_666666"
                android:textSize="@dimen/sp_13" />


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_25"
                android:paddingStart="@dimen/dp_15"
                android:paddingEnd="@dimen/dp_15"
                android:text="@string/office_address"
                android:textColor="@color/black_333333"
                android:textSize="@dimen/sp_17"
                android:textStyle="bold" />


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_12"
                android:lineSpacingMultiplier="1.2"
                android:paddingStart="@dimen/dp_15"
                android:paddingEnd="@dimen/dp_15"
                android:text="@string/office_address_detail"
                android:textColor="@color/grey_666666"
                android:textSize="@dimen/sp_13" />


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_25"
                android:paddingStart="@dimen/dp_15"
                android:paddingEnd="@dimen/dp_15"
                android:text="@string/quick_links"
                android:textColor="@color/black_333333"
                android:textSize="@dimen/sp_17"
                android:textStyle="bold" />


            <TextView
                android:id="@+id/home_page_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_12"
                android:lineSpacingMultiplier="1.2"
                android:paddingStart="@dimen/dp_15"
                android:paddingEnd="@dimen/dp_15"
                android:textColor="@color/grey_666666"
                android:textSize="@dimen/sp_13"
                tools:text="Website homepage：https://wte.where.ph/#to-home" />


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_25"
                android:paddingStart="@dimen/dp_15"
                android:paddingEnd="@dimen/dp_15"
                android:text="@string/contact_us"
                android:textColor="@color/black_333333"
                android:textSize="@dimen/sp_17"
                android:textStyle="bold" />


            <TextView
                android:id="@+id/call_us_tv"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_12"
                android:lineSpacingMultiplier="1.2"
                android:paddingStart="@dimen/dp_15"
                android:paddingEnd="@dimen/dp_15"
                android:textColor="@color/grey_666666"
                android:textSize="@dimen/sp_13"
                android:text="@string/call_us" />


            <TextView
                android:id="@+id/message_us_tv"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_12"
                android:lineSpacingMultiplier="1.2"
                android:paddingStart="@dimen/dp_15"
                android:paddingEnd="@dimen/dp_15"
                android:textColor="@color/grey_666666"
                android:textSize="@dimen/sp_13"
                android:text="@string/message_us" />


            <TextView
                android:id="@+id/email_us_tv"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_12"
                android:lineSpacingMultiplier="1.2"
                android:paddingStart="@dimen/dp_15"
                android:paddingEnd="@dimen/dp_15"
                android:textColor="@color/grey_666666"
                android:textSize="@dimen/sp_13"
                android:text="@string/email_us" />


            <LinearLayout
                android:id="@+id/debug_into_ll"
                android:layout_marginTop="@dimen/dp_500"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:visibility="gone"
                tools:visibility="visible">


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_25"
                    android:paddingStart="@dimen/dp_15"
                    android:paddingEnd="@dimen/dp_15"
                    android:text="调试信息(仅在测试服出现)"
                    android:textColor="@color/black_333333"
                    android:textSize="@dimen/sp_17"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/user_id_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_15"
                    android:lineSpacingMultiplier="1.2"
                    android:paddingStart="@dimen/dp_15"
                    android:paddingEnd="@dimen/dp_15"
                    android:text="User Id："
                    android:textColor="@color/grey_666666"
                    android:textSize="@dimen/sp_13" />

                <TextView
                    android:id="@+id/user_phone_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_15"
                    android:lineSpacingMultiplier="1.2"
                    android:paddingStart="@dimen/dp_15"
                    android:paddingEnd="@dimen/dp_15"
                    android:text="User Phone："
                    android:textColor="@color/grey_666666"
                    android:textSize="@dimen/sp_13" />


                <TextView
                    android:id="@+id/rong_token_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_15"
                    android:lineSpacingMultiplier="1.2"
                    android:paddingStart="@dimen/dp_15"
                    android:paddingEnd="@dimen/dp_15"
                    android:text="融云Token："
                    android:textColor="@color/grey_666666"
                    android:textSize="@dimen/sp_13" />
                <TextView
                    android:id="@+id/rong_uuid_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_15"
                    android:lineSpacingMultiplier="1.2"
                    android:paddingStart="@dimen/dp_15"
                    android:paddingEnd="@dimen/dp_15"
                    android:text="融云UUID："
                    android:textColor="@color/grey_666666"
                    android:textSize="@dimen/sp_13" />

                <TextView
                    android:id="@+id/push_id_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_15"
                    android:lineSpacingMultiplier="1.2"
                    android:paddingStart="@dimen/dp_15"
                    android:paddingEnd="@dimen/dp_15"
                    android:text="极光RegistrationId："
                    android:textColor="@color/grey_666666"
                    android:textSize="@dimen/sp_13" />


                <TextView
                    android:id="@+id/umeng_channel_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_15"
                    android:lineSpacingMultiplier="1.2"
                    android:paddingStart="@dimen/dp_15"
                    android:paddingEnd="@dimen/dp_15"
                    android:text="友盟Channel："
                    android:textColor="@color/grey_666666"
                    android:textSize="@dimen/sp_13" />


                <TextView
                    android:id="@+id/user_all_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_15"
                    android:lineSpacingMultiplier="1.2"
                    android:paddingStart="@dimen/dp_15"
                    android:paddingEnd="@dimen/dp_15"
                    android:text="User："
                    android:textColor="@color/grey_666666"
                    android:textSize="@dimen/sp_8" />

            </LinearLayout>


        </LinearLayout>


    </androidx.core.widget.NestedScrollView>


</LinearLayout>