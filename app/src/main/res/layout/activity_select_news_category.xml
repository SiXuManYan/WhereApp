<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.jcs.where.widget.JcsTitle
        android:id="@+id/jcsTitle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:backIcon="@mipmap/ic_close_black"
        app:backIconHeight="@dimen/dp_14"
        app:backIconWidth="@dimen/dp_14"
        app:layout_constraintTop_toTopOf="parent"
        app:middleTitle="" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/channelRecycler"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/dp_15"
        android:layout_marginEnd="@dimen/dp_15"
        app:layout_constraintTop_toBottomOf="@id/jcsTitle"
        android:layout_marginTop="@dimen/dp_15"/>


</androidx.constraintlayout.widget.ConstraintLayout>