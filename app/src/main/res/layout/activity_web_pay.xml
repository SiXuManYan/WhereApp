<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">



    <com.jiechengsheng.city.widget.JcsTitle
        android:id="@+id/jcsTitle"
        app:backIcon="@mipmap/ic_back_delete"
        android:layout_width="match_parent"
        app:backgroundRes="@mipmap/ic_back_delete"
        android:layout_height="wrap_content"
        app:middleTitle="@string/pay_order_title" />


    <com.jiechengsheng.city.features.web.JsWebView
        android:id="@+id/web_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/jcsTitle" />


    <com.jiechengsheng.city.view.empty.EmptyView
        android:id="@+id/empty_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/jcsTitle"
        android:background="@color/white"
        android:visibility="gone"
        tools:visibility="visible" />


</RelativeLayout>