<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/store_cart_ll"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:orientation="vertical">


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_48">

        <CheckedTextView
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dp_48"
            android:gravity="center_vertical|end"
            android:paddingStart="@dimen/dp_15"
            android:paddingEnd="@dimen/dp_15"
            android:text="@string/expired_products"
            android:textColor="@color/black_333333"
            android:textSize="@dimen/sp_14" />


        <CheckedTextView
            android:id="@+id/delete_expired_tv"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dp_48"
            android:layout_alignParentEnd="true"
            android:drawableEnd="@mipmap/ic_delete_all_gray"
            android:drawablePadding="@dimen/dp_4"
            android:gravity="center_vertical|end"
            android:paddingStart="@dimen/dp_15"
            android:paddingEnd="@dimen/dp_15"
            android:text="@string/delete_all"
            android:textColor="@color/grey_666666"
            android:textSize="@dimen/sp_12" />


    </RelativeLayout>



    <LinearLayout
        android:id="@+id/child_container_ll"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <include layout="@layout/item_shopping_cart_child_for_store_mall_expired" />

    </LinearLayout>


</LinearLayout>