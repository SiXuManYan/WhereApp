<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/store_cart_ll"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:orientation="vertical">


    <CheckedTextView
        android:id="@+id/select_all_tv"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_48"
        android:drawableStart="@drawable/selector_store_cart_select_all"
        android:drawablePadding="@dimen/dp_10"
        android:gravity="center_vertical|end"
        android:paddingStart="@dimen/dp_15"
        android:paddingEnd="@dimen/dp_15"
        android:textColor="@color/black_333333"
        android:textSize="@dimen/sp_14"
        tools:text="糖糖" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/grey_F5F5F5" />


    <LinearLayout
        android:id="@+id/child_container_ll"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <include layout="@layout/item_shopping_cart_child_for_store" />

    </LinearLayout>


</LinearLayout>